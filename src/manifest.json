{
  "manifest_version": 3,
  "name": "Boore",
  "description": "very cool extension",
  "version": "1.0.0",
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
        "16": "/images/icon-16x16.png",
        "128": "/images/icon-128x128.png"
    },
  "action": {
    "default_icon": "images/icon-16x16.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "scripting"
    ]
}

